\section{Performance}
\begin{frame}{The used hardware}{}
	\begin{block}{The network interface cards}
		\begin{itemize}
	  		\item Four cards with Intel  82541PI controller (PCI on PCI-X)
	  		\item Two dualport cards Broadcom BCM5751 (PCIe 4x)
	  		\item Two dualport cards Intel 82546GB (PCI-X)
		\end{itemize}
	\end{block}
	
	\begin{block}{The machines}
    	\centering
    	\begin{itemize}
		  \item Quadcore Intel® Xeon® processor 5100 @ \textbf{1,6GHz}
		\end{itemize}
    \end{block}
	
	\begin{ergo}
		Two machines with four direct attached links on each (no switch!)
	\end{ergo}
\end{frame}

